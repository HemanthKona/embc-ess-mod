{
  "name": "embc-responder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-prj": "ng serve --port 6200 --proxy-config proxy.dev.project.conf.json",
    "start-sup": "ng serve --port 6200 --proxy-config proxy.dev.support.conf.json",
    "start": "ng serve --port 6200 --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier . --write",
    "gen-api-prj": "ng-openapi-gen -i https://dev-era-responders.apps.silver.devops.gov.bc.ca/api/openapi/v1/openapi.json -o ./src/app/core/api --ignoreUnusedModels false &&  && npm run format:write",
    "gen-api-sup": "ng-openapi-gen -i https://era-responders-dev.apps.silver.devops.gov.bc.ca/api/openapi/v1/openapi.json -o ./src/app/core/api --ignoreUnusedModels false &&  && npm run format:write",
    "gen-api-local": "ng-openapi-gen -i http://localhost:6020/api/openapi/v1/openapi.json -o ./src/app/core/api --ignoreUnusedModels false &&  && npm run format:write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.2.12",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "~16.2.12",
    "@angular/compiler": "~16.2.12",
    "@angular/core": "~16.2.12",
    "@angular/forms": "~16.2.12",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "~16.2.12",
    "@angular/platform-browser-dynamic": "~16.2.12",
    "@angular/router": "~16.2.12",
    "@ng-idle/core": "^14.0.0",
    "@ng-idle/keepalive": "^14.0.0",
    "angular-imask": "^7.6.0",
    "angular-oauth2-oidc": "^14.0.1",
    "bootstrap": "~5.1.3",
    "moment": "^2.29.4",
    "ngx-markdown": "^16.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.2.14",
    "@angular-eslint/builder": "^16.3.1",
    "@angular-eslint/eslint-plugin": "^16.3.1",
    "@angular-eslint/eslint-plugin-template": "~16.3.1",
    "@angular-eslint/schematics": "~16.3.1",
    "@angular-eslint/template-parser": "~16.3.1",
    "@angular/cli": "~16.2.14",
    "@angular/compiler-cli": "~16.2.12",
    "@types/jasmine": "^3.10.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.5",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^39.9.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-openapi-gen": "^0.51.0",
    "prettier": "^3.2.5",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "typescript": "4.9.3"
